{
  "builds": [
    { "src": "views/index.html", "use": "@vercel/static" },
    { "src": "css/index.css", "use": "@vercel/static" },
    { "src": "js/index.js", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "views/index.html" },
    { "src": "/index.css", "dest": "css/index.css" },
    { "src": "/index.js", "dest": "js/index.js" }
  ]
}