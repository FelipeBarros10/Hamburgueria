{
  "builds": [
    { "src": "src/views/index.html", "use": "@vercel/static" },
    { "src": "src/css/index.css", "use": "@vercel/static" },
    { "src": "src/js/index.js", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "src/html/index.html" },
    { "src": "/index.css", "dest": "src/css/index.css" },
    { "src": "/index.js", "dest": "src/js/index.js" }
  ]
}